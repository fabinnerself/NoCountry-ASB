{
  "name": "autostory-builder-phase1",
  "version": "1.0.0",
  "description": "AutoStory Builder - Fase 1: Image Processing & Story Generation",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/img/unit",
    "test:unit:fast": "jest tests/img/unit --testPathIgnorePatterns=imageAnalyzer.test.ts --testPathIgnorePatterns=storyGenerator.test.ts",
    "test:integration": "jest tests/img/integration",
    "test:e2e": "jest tests/img/e2e",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "ai",
    "story",
    "generator",
    "cohere",
    "vision",
    "image-processing"
  ],
  "author": "AutoStory Builder Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cohere-ai": "^7.14.0",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/cors": "^2.8.17",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "form-data": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}